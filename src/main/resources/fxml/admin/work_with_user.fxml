<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.kurilenko.controller.admin.users.AdminWorkWithUserController">
   <children>
      <BorderPane prefHeight="400.0" prefWidth="600.0">
         <bottom>
            <HBox alignment="CENTER" prefHeight="50.0" prefWidth="200.0" BorderPane.alignment="CENTER">
               <children>
                  <JFXButton fx:id="btnBack" alignment="CENTER" contentDisplay="CENTER" onMouseClicked="#btnBackMMouseClicked" style="-fx-background-color: #757de8;" text="Назад">
                     <font>
                        <Font name="Verdana" size="14.0" />
                     </font>
                     <opaqueInsets>
                        <Insets />
                     </opaqueInsets>
                     <HBox.margin>
                        <Insets right="10.0" />
                     </HBox.margin>
                  </JFXButton>
                  <JFXButton fx:id="btnAddUser" onMouseClicked="#btnAddUserMouseClicked" style="-fx-background-color: #757de8;" text="Добавить пользователя">
                     <font>
                        <Font name="Verdana" size="14.0" />
                     </font>
                     <HBox.margin>
                        <Insets right="10.0" />
                     </HBox.margin>
                  </JFXButton>
                  <JFXButton fx:id="btnDelUser" style="-fx-background-color: #757de8;" text="Удалить">
                     <font>
                        <Font name="Verdana" size="14.0" />
                     </font>
                     <HBox.margin>
                        <Insets right="10.0" />
                     </HBox.margin>
                  </JFXButton>
                  <JFXButton fx:id="btnEditUser" style="-fx-background-color: #757de8;" text="Изменить">
                     <font>
                        <Font name="Verdana" size="14.0" />
                     </font>
                  </JFXButton>
               </children>
            </HBox>
         </bottom>
         <center>
            <JFXTreeTableView fx:id="tableUsers" prefHeight="400.0" prefWidth="420.0" BorderPane.alignment="CENTER" />
         </center>
      </BorderPane>
   </children>
</AnchorPane>
